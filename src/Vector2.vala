public class Vector2
{
	private float X;
	private float Y;

	public Vector2(float x, float y)
	{
		X = x;
		Y = y;
	}

	public float getX()
	{
		return X;
	}

	public float getY()
	{
		return Y;
	}

	public void setX(float x)
	{
		X = x;
	}

	public void setY(float y)
	{
		Y = y;
	}

	public void set(float x, float y)
	{
		X = x;
		Y = y;
	}
}
